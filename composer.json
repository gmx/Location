        "geolocation",
        "gis",
        "geo location",
        "geospatial"
    ],
    "license":"BSD-3-Clause",
    "authors":[{
        "name":"Rick Ogden",
        "email":"rick@rickogden.com",
        "homepage":"http://www.rickogden.com",
        "role":"developer"
    }],
    "require": {
        "php": ">=5.4|^7.0"
    },
    "require-dev": {
